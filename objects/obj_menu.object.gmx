            <string>/// Menu Selector Code

var move = 0;
move -= max(keyboard_check_pressed(ord("W")),0);
move += max(keyboard_check_pressed(ord("S")),0);

if (move != 0){
    mpos += move;
    if (mpos &lt; 0) mpos = array_length_1d(menu) - 1;
    if (mpos &gt; array_length_1d(menu) -1) mpos = 0;
    var snd = audio_play_sound(snd_laser_01, 10, false);
    audio_sound_gain(snd, 0.5, 0);
}

var push;
push = max(keyboard_check_released(vk_enter),max(keyboard_check_released(vk_space)), 0)

if (push == 1){
    scr_menu()
    var snd = audio_play_sound(snd_explode_01, 10, false);
    audio_sound_gain(snd, 0.5, 0);
}
</string>
